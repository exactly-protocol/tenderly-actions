{
  "name": "@exactly-protocol/tenderly-actions",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "repository": "github:exactly-protocol/tenderly-actions",
  "scripts": {
    "lint": "eslint .",
    "test:mocha": "mocha",
    "test": "run-s --continue-on-error lint test:*",
    "prebuild": "ts-node scripts/prebuild.ts",
    "upgrade": "run-s upgrade:* && npm install",
    "upgrade:ncu": "ncu -u && ncu --cwd actions -u",
    "upgrade:clean": "rimraf '{.,actions}/node_modules' '{.,actions}/package-lock.json'",
    "postinstall:patch": "patch-package",
    "postinstall:actions": "npm --prefix actions install",
    "postinstall:scripts": "ts-node scripts/postinstall.ts",
    "postinstall": "run-s postinstall:* prebuild"
  },
  "files": [
    "actions"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@exactly-protocol/protocol": "^0.2.5",
    "@tenderly/actions-test": "^0.1.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/eslint": "^8.21.1",
    "@types/mocha": "^10.0.1",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "glob-promise": "^6.0.2",
    "mocha": "^10.2.0",
    "mustache": "^4.2.0",
    "npm-check-updates": "^16.7.9",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  }
}
